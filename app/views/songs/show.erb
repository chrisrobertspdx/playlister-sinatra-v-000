<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1><%= @song.name %></h1>
Artist Name: <%= @song.artist.name %>
<h3>Genres</h3>
<ul>
<% @song.genres.each do |g| %>
<li><a href="/genres/<%= g.slug %>"><%= g.name %></a></li>
<% end %>
</ul>
<a href="/songs/<%= @song.slug %>/edit">Edit</a>
